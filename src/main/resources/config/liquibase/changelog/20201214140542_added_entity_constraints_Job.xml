<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity Job.
    -->
    <changeSet id="20201214140542-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="job_id"
                                 baseTableName="rel_job__worked"
                                 constraintName="fk_rel_job__worked__job_id"
                                 referencedColumnNames="id"
                                 referencedTableName="job"/>

        <addForeignKeyConstraint baseColumnNames="worked_id"
                                 baseTableName="rel_job__worked"
                                 constraintName="fk_rel_job__worked__worked_id"
                                 referencedColumnNames="id"
                                 referencedTableName="worked"/>

        <addForeignKeyConstraint baseColumnNames="machine_id"
                                 baseTableName="job"
                                 constraintName="fk_job__machine_id"
                                 referencedColumnNames="id"
                                 referencedTableName="machine"/>
    </changeSet>
</databaseChangeLog>
